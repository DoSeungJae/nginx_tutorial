server {
        listen 83;
        server_name nginxtutorial1192.duckdns.org;  

        root /srv/nginx-handbook-projects/static-demo;

    listen 445 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/nginxtutorial1192.duckdns.org/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/nginxtutorial1192.duckdns.org/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}